                    </tr>
                    <tr>
                        <td>0</td>
                        <td>Extreme</td>
                        <td>Accept</td>
                        <td>Accepted</td>
                        <td>Pass</td>
                    </tr>
                     <tr>
                        <td>100</td>
                        <td></td>
                        <td>Accept</td>
                        <td>Accepted</td>
                        <td>Pass</td>
                    </tr>
                     <tr>
                        <td>-80</td>
                        <td>Abnormal</td>
                        <td>Reject</td>
                        <td>Rejected</td>
                        <td>Pass</td>
                    </tr>
                </tbody>
